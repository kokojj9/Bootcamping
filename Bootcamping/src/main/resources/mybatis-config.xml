<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.bootcamping.member.model.vo.Member" alias="member"/>
		<typeAlias type="com.kh.bootcamping.camping.model.vo.Camping" alias="camping"/>
		<typeAlias type="com.kh.bootcamping.camping.model.vo.Site" alias="site" />
		<typeAlias type="com.kh.bootcamping.review.model.vo.Review" alias="review" />
	</typeAliases>
	
	
	<mappers>
		<mapper resource="/mapper/member/member-mapper.xml"/>
		<mapper resource="/mapper/camping/camping-mapper.xml"/>
		<mapper resource="/mapper/review/review-mapper.xml"/>
	</mappers>
	

</configuration>
